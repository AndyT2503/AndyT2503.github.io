    "@angular/platform-browser-dynamic": "^17.1.3",
    "@angular/router": "^17.1.3",
    "@storybook/angular": "^7.2.1",
    "markdown-reading-time": "^0.1.1",
    "ng-zorro-antd": "^17.2.0",
    "ngx-markdown": "^16.0.0",
